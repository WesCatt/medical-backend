generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Data {
  id                    Int     @id @default(autoincrement())
  session_order_id      String
  round_in_session      Int
  user_message          String
  agent_message         String
  doctor_message        String
  consultation_status   Int
  disease_predictions   Json
  consultation_elements Json
  rag_retrieval         Json
  end_tag               Boolean
  status_code           Int
  error_msg             String?

  @@unique([session_order_id,round_in_session])
}
